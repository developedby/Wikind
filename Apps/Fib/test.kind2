Main : _ {
  // Initial State
  let state  = Apps.Fib.State.init
  // Player 0
  // Pos  = 141 : 15
  // Exp  = 3251
  // Hp   = 100
  // Spls = Fireball
  let p0     = Apps.Fib.Player.new 141u120 15u120 3251u120 100u120 0u120 0u120 0u120 [1u120]
  // Player 1
  // Pos  = 144 : 10
  // Exp  = 1423
  // Hp   = 100
  // Spls = Fireball 
  let p1     = Apps.Fib.Player.new 144u120 10u120 1423u120 100u120 0u120 0u120 0u120 [1u120]
  // Id for player 0
  let p0_id  = 1017u120
  // Id for player 1
  let p1_id  = 625u120
  // Pos for player 0 
  let p0_pos = Apps.Fib.Pos.new 141u120 15u120
  // Pos for player 1
  let p1_pos = Apps.Fib.Pos.new 144u120 10u120
  // TODO: change this to create a character from an action
  // Places player 0 on the state
  let state  = Apps.Fib.State.player.place state p0 p0_id
  // Places player 1 on the state
  let state  = Apps.Fib.State.player.place state p1 p1_id
  // Shows All players in the Table
  let t_shw0 = Apps.Fib.Table.show_players (Apps.Fib.test.Table state)
  // Action code for Player 0 casting Fireball on Player 1
  // 0u120 stands for the first skill in a character
  // let code_0 = Apps.Fib.Action.skill_code state 0u120 p0_id p1_pos
  // let code_0 = Apps.Fib.Interact.skill p0_id 0u120 3u120 13u120
  let code_0 = Apps.Fib.Interact.pos.skill p0_id 0u120 (Apps.Fib.Pos.get_x p0_pos) (Apps.Fib.Pos.get_y p0_pos) (Apps.Fib.Pos.get_x p1_pos) (Apps.Fib.Pos.get_y p1_pos)
  // Player 2 Id
  let p2_id  = 780u120
  // Action code to create a new character in a town
  // 0 = top left
  // 1 = top right
  // 2 = bot right
  // 3 = bot left
  let code_1 = Apps.Fib.Interact.spawn p2_id 1u120
  // Action code to walk to a point (max range = 2)
  // let code_2 = Apps.Fib.Action.walk_code state p0_id (Apps.Fib.Pos.new 141u120 17u120)
  // Action code to walk a direction 
  let code_2 = Apps.Fib.Interact.walk p0_id 9u120 9u120
  // let 
  // Runs Fireball action code in the state
  let state  = Apps.Fib.State.update state code_0
  let state  = Apps.Fib.State.update state code_0 
  // Runs Spawn action code in the state
  let state  = Apps.Fib.State.update state code_1
  // Runs walk action code in the state
  let state  = Apps.Fib.State.update state code_2
  // Runs walk action code in the state
  // let state  = Apps.Fib.State.update state code_3
  // Shows a certain part of the map
  // Starting x = 130
  // Starting y = 0
  // Width      = 20
  // Height     = 20
  let m_show = Apps.Fib.Map.show 130u120 0u120 30u120 20u120 (Apps.Fib.test.Map state)
  // Shows all players in the Table
  let t_shw1 = Apps.Fib.Table.show_players (Apps.Fib.test.Table state)
  // let j = Apps.Fib.Table.got (Apps.Fib.test.Table state) p0_id
  // let jsnd = Pair.snd j
  // Show.to_string (U120.show jsnd)
  // state
  // m_show
  t_shw1
}


