#kdl_name = Fib_itp_sk
Apps.Fib.Interact.pos.skill (id: Apps.Fib.Id) (skill: U120) (cx: U120) (cy: U120) (tx: U120) (ty: U120) : U120 { 
  let comm    = (U120.add 2u120 skill)
  let data    = Apps.Fib.Interact.pos.aux cx cy tx ty
  let action  = (U120.add id (U120.add (U120.shift_left comm 12u120) (U120.shift_left data 16u120)))
  action
}
