#kdl_name = Fib_itp_wk
Apps.Fib.Interact.pos.walk (id: Apps.Fib.Id) (cx: U120) (cy: U120) (tx: U120) (ty: U120) : U120 {
  let comm    = 0u120
  let data    = Apps.Fib.Interact.pos.aux cx cy tx ty
  let action  = (U120.add id (U120.add (U120.shift_left comm 12u120) (U120.shift_left data 16u120)))
  action
}