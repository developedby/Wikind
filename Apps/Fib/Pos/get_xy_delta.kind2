#kdl_name = Fib_pos_g_xy
Apps.Fib.Pos.get_xy_delta (data: U120) : Pair U120 U120 {
  let dx = U120.bitwise_and data 15u120
  let dy = U120.shift_right data 4u120
  let func =
    (n: U120) =>
      if (U120.equal (U120.div n 8u120) 1u120) {
        U120.sub 0u120 (U120.mod n 8u120)
      }  
      else {
        U120.mod n 8u120
      }
  Pair.new (func dx) (func dy)
}
