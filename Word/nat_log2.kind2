Word.nat_log2 <size: Nat> (word: Word size) : Nat
Word.nat_log2 size word = Word.nat_log2.go word Nat.zero Nat.zero

Word.nat_log2.go <size: Nat> (word: Word size) (c: Nat) (n: Nat) : Nat
Word.nat_log2.go s  Word.e          c n = n
Word.nat_log2.go s (Word.o s_ pred) c n = Word.nat_log2.go pred (Nat.succ c) n
Word.nat_log2.go s (Word.i s_ pred) c n = Word.nat_log2.go pred (Nat.succ c) c