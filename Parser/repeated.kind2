// Applies a parser repeatedly until it fails
// Returns a list of all results
Parser.repeated <a> (prs: Parser a) : Parser (List a) {
  do Parser {
    ask mh = Parser.try prs
    match Maybe mh {
      none => Parser.pure []
      some => do Parser {
        ask tail = Parser.until_fail prs
        return List.cons mh.value tail
      }
    }
  }
}