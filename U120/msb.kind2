// Return the index of the most significant set bit
U120.msb (n: U120) : Maybe U120
U120.msb 0u120 = Maybe.none
U120.msb n     = Maybe.some (U120.sub (U120.msb.go n) 1u120)


U120.msb.go (n: U120) : U120
U120.msb.go 0u120 = 0u120
U120.msb.go n     = U120.add 1u120 (U120.msb.go (U120.shift_right n 1u120))