// Returns the length of the bits without leading 0s
// Example: (Bits.size (Bits.i (Bits.o Bits.e))) -> 1
Bits.size(bits: Bits): Nat {
  Bits.size.go bits Nat.zero Nat.zero
}

Bits.size.go (bits: Bits) (c: Nat) (n: Nat) : Nat
Bits.size.go  Bits.e    c n = n
Bits.size.go (Bits.o b) c n = Bits.size.go b (Nat.succ c) n
Bits.size.go (Bits.i b) c n = Bits.size.go b (Nat.succ c) (Nat.succ c)