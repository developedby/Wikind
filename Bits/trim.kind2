// Removes leading zeros
// Example: (Bits.trim (Bits.o (Bits.i (Bits.o Bits.e)))) -> (Bits.o (Bits.i Bits.e))
Bits.trim (b: Bits) : Bits {
  Bits.trim.go b Bits.e Bits.e
}

Bits.trim.go (b: Bits) (acc: Bits) (res: Bits) : Bits
Bits.trim.go  Bits.e     acc res = res
Bits.trim.go (Bits.o bp) acc res = Bits.trim.go bp (Bits.o acc) res
Bits.trim.go (Bits.i bp) acc res = Bits.trim.go bp (Bits.i acc) (Bits.i acc)